@model VuDucLapConnModel.Patient

@{
    // Layout = null;
    ViewBag.Title = "Index";
}

<h2 align="center">KẾT QUẢ XÉT NGHIỆM</h2>

<p>
  Họ tên :   @Html.DisplayFor(model=>model.PatientName)
</p>

<p>
    @if (Model.Sex == "F")
    {
        
        @Html.Raw("Giới tính : Nữ")
    }
    else
    {
        @Html.Raw("Giới tính : Nam")
    }
</p>

<p>
    Năm sinh :@Html.DisplayFor(model => model.Age)
</p>
<p>
    Địa chỉ : @Html.DisplayFor(model => model.Address)
</p>

<table class="table">
    <tr>
        @*<th>
            @Html.DisplayNameFor(model => model.TestResults.SID)
        </th>*@
        @*<th>
            Test Code
        </th>*@
        <th>
           Tên xét nghiệm
        </th>
        <th>
          Kết quả
        </th>
      
        @*<th>
            @Html.DisplayNameFor(model => model.ResultTime)
        </th>*@
        <th>
           Đơn vị   
        </th>
        <th>
           Trị số tham chiếu
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Category)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdateTime)
        </th>*@
        <th></th>
    </tr>

    @foreach (var item in Model.TestResults)
    {
        <tr>
           
            @*<td>
                @Html.DisplayFor(modelItem => item.Testcode)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.TestName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Result)
            </td>

            @*<td>
                    @Html.DisplayFor(modelItem => item.ResultTime)
                </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Unit)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NormalRange)
            </td>
            @*<td>
                    @Html.DisplayFor(modelItem => item.Category)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UpdateTime)
                </td>*@

        </tr>
    }

</table>
